{
  "name": "ontographia-diagram-studio",
  "version": "1.0.0",
  "private": true,
  "description": "Ontographia Diagram Studio - Visual diagramming workspace",
  "scripts": {
    "dev": "next dev",
    "dev:test": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --selectProjects unit",
    "test:watch": "jest --selectProjects unit --watch",
    "test:coverage": "jest --selectProjects unit --coverage",
    "test:e2e": "jest --selectProjects e2e --runInBand",
    "test:e2e:headed": "HEADLESS=false jest --selectProjects e2e --runInBand",
    "test:all": "jest --runInBand",
    "db:init": "node scripts/init-db.js",
    "db:seed": "node scripts/seed-admin.js",
    "db:seed:list": "node scripts/seed-admin.js list",
    "db:setup": "npm run db:init && npm run db:seed"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "bcryptjs": "^3.0.3",
    "date-fns": "^4.1.0",
    "next": "^16.1.1",
    "next-auth": "^4.24.13",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "puppeteer": "^24.34.0"
  }
}
